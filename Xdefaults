! urxvt font configuration:
URxvt*boldColors: on
URxvt*font:       xft:liberation mono:pixelsize=15:antialias=true:hinting=true:medium
URxvt*boldFont:   xft:liberation mono:pixelsize=15:antialias=true:hinting=true:bold

! urxvt extensions:
URxvt.perl-lib: /home/plamen/.urxvt
URxvt.perl-ext-common: clipboard

URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.clipboard.copycmd:  xclip -i -selection clipboard
URxvt.clipboard.pastecmd: xclip -o -selection clipboard
